{
   "background": {
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "/assets/umber.png"
   },
   "browser_specific_settings": {
      "gecko": {"id": "41@neocities.org"}
   },
   "content_scripts": [
      {
         "js": [ "content.js" ],
         "matches": ["<all_urls>"]
      }
   ],
   "manifest_version": 2,
   "name": "Umber",
   "permissions": [
      "https://bandcamp.com/",
      "https://api-v2.soundcloud.com/",
      "https://www.youtube.com/"
   ],
   "version": "1.3.3",
   "web_accessible_resources": [
      "/assets/umber.png",
      "/assets/umber.css"
   ]
}
